{
    "version": 2,
    "builds": [
      {
        "src": "dist/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/health",
        "dest": "dist/server.js"
      },
      {
        "src": "/api/diagnostic",
        "dest": "dist/server.js"
      },
      {
        "src": "/api/auth/(.*)",
        "dest": "dist/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "dist/server.js"
      }
    ]
  }